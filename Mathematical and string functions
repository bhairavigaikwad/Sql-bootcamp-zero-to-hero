/* MATHEMATICAL FUNCS
SELECT ROUND(rental_rate/replacement_cost,2)*100 AS percent_cost
FROM film

SELECT 0.1*replacement_cost 
FROM film */

/* STRING FUNCS 
SELECT length(first_name)
FROM customer 

SELECT UPPER(first_name) || ' ' ||last_name AS full_name
FROM customer 

SELECT LOWER(LEFT(first_name,1)) || last_name || '@gmail.com'
AS custom_email
FROM customer */



