/* TASK 2*/
SELECT title, first_name, last_name
FROM film_actor
INNER JOIN actor ON
actor.actor_id=film_actor.actor_id
INNER JOIN film ON
film_actor.film_id=film.film_id
WHERE first_name= 'Nick' AND last_name='Wahlberg'
